{
    "compilerOptions": {
      "target": "es2020",
      "module":"ES6",
      "moduleResolution":"Node",
      "resolveJsonModule": true,
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
    },
    "include": ["src/**/*"],
    "exclude": ["src/types/*.ts", "node_modules"]
  }